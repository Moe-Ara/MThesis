<group name="custom_rules,sysmon,">
  <rule id="100505" level="10">
    <if_group>sysmon</if_group>
    <field name="win.eventdata.image">powershell.exe</field>
    <!-- Check if the command line contains 'New-Process' -->
    <match>.*New-Process.*</match>
    <description>Suspicious PowerShell usage creating a new process.</description>
    <group>sysmon,windows,powershell_newprocess,</group>
  </rule>
</group>
